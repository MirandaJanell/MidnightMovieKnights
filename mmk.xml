{% for post in site.categories.podcast limit:20 %}
	{% assign episodeurl = post.link | replace: 'http://', 'http://www.podtrac.com/pts/redirect.m4a/' %}
	<item>
		<title>{{ post.title }}</title>
		<itunes:author>@MirandaJanell &amp; @ThatFrakkingCat</itunes:author>
		<itunes:subtitle>{{ post.subtitle }}</itunes:subtitle>
		<itunes:summary>{{ post.content | strip_html | xml_escape }}</itunes:summary>
		<itunes:image href="http://media.midnightmovieknights.com/img/logo1-1400x1400.jpg" />
		<enclosure url="{{ episodeurl }}" length="{{ post.length }}" type="audio/mp4" />
		<guid>{{ episodeurl }}</guid>
		<pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S" }} PST</pubDate>
		<itunes:duration>{{ post.duration | remove: '.0' }}</itunes:duration>
		<itunes:keywords>{% for tag in post.tags %}{{ tag }},{% endfor %}Midnight,Movie,Knights,Midnight Movie Knights,MirandaJanell,ThatFrakkingCat</itunes:keywords>
		<itunes:explicit>Yes</itunes:explicit>
	</item>
{% endfor %}
